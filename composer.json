{
    "name": "twocabbages/yii2-builder",
    "description": "Build forms (single-row or multi-row/tabular) easily for Yii Framework 2.0",
    "keywords": ["yii2", "extension", "widget", "form", "builder"],
    "homepage": "https://github.com/twocabbages/yii2-builder",
    "type": "yii2-extension",
    "license": "BSD 3-Clause",
    "authors": [
        {
            "name": "Kartik Visweswaran",
            "email": "kartikv2@gmail.com",
            "homepage": "http://www.krajee.com/"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "kartik-v/yii2-widgets": "*",
        "kartik-v/yii2-grid": "*"
    },
    "autoload": {
        "psr-4": {
            "kartik\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5.x-dev"
        }
    }
}
